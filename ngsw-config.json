{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/firebase-messaging-sw.js",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }

    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "details",
      "cacheConfig": {
        "maxAge": "1d",
        "maxSize": 100,
        "strategy": "freshness",
        "timeout": "10s"
      },
      "urls": ["**/basicdetails","**/points_details"],
      "version": 1
    }
  ]
}
